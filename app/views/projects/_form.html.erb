<%= form_with(model: project, local: true) do |form| %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :worked_hours %>
    <%= form.text_field :worked_hours %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
