<% provide(:title, @group.name) %>
<p id="notice"><%= notice %></p>
<div class="row">
  <aside class="col-md-4">
    <section class="group_info">
      <h1>
        <span><%= image_tag @group.icon.url if @group.icon? %></span>
        <%= @group.name %>
      </h1>
    </section>
  </aside>
  <div class="col-md-8">
    <div>
      <%= link_to 'All my Projects', projects_path %> 
    </div>

    <div>
      <%= link_to 'All Projects Shared with me', "#" %> 
    </div>

    <div>
    <%= link_to 'All my Groups', groups_path %>  
    </div>
  </div>
</div>
